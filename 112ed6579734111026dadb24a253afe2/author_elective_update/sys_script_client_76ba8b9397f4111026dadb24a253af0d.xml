        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Get contry info by name</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }
  if (newValue.length > 0) {
        var ga = new GlideAjax('GettheCountryInfo');
        ga.addParam('sysparm_name', 'GettheCountryInfo');
        ga.addParam('sysparm_personName', newValue);
        ga.getXML(updateFields);

    }

    function updateFields(response) {
        var answer = response.responseXML.documentElement.getAttribute("answer");

        if (answer) {
            var returneddata = JSON.parse(answer);
            g_form.setValue('predicted_gender', returneddata.gender.gender);
            g_form.setValue('count_of_times_the_name_was_predicted', returneddata.gender.count);
            g_form.setValue('predicted_gender_probability', returneddata.gender.probability);

            for (var i = 0; i < 3; i++) {
                if (i == 0) {
                    g_form.setValue('most_likely_country_probability', returneddata.country.country[i].country_id);
                    g_form.setValue('most_likely_country_code', returneddata.country.country[i].probability);
                } else if (i == 1) {
                    g_form.setValue('second_most_likely_country_probability', returneddata.country.country[i].country_id);
                    g_form.setValue('second_most_likely_country_code', returneddata.country.country[i].probability);
                } else if (i == 2) {
                    g_form.setValue('third_most_likely_country_probability', returneddata.country.country[i].country_id);
                    g_form.setValue('third_most_likely_country_code', returneddata.country.country[i].probability);
                }

            }
        }
    }
}]]></script>
