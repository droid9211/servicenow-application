        <field>u_final_price</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Sum of price quantity</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    if (newValue != oldValue && newValue != '') {



        var f1 = g_form.getValue('u_related_order_lines'); // give quantity name

        var f2 = g_form.getValue('u_price'); //give value name

        if (f1 != '' && f2 != '') {

            g_form.setValue('u_final_price', parseInt(f1) * parseInt(f2)); //give total name

        }

    }
}]]></script>
