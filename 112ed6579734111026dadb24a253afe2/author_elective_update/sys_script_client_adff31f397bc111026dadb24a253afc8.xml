        <field>u_related_order_lines</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Calculate total price</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }
	var productsSelected = g_form.getValue('u_related_order_lines');
	var priceA = new GlideAjax('TotalPrice');
	priceA.addParam('sysparm_name', 'Price');
	priceA.addParam('sysparm_productsArray', productsSelected);
	priceA.getXML(setPrice);
	
	function setPrice(response) {
		var answer = response.responseXML.documentElement.getAttribute("answer");
		g_form.setValue('total_price', answer);
	}
}]]></script>
