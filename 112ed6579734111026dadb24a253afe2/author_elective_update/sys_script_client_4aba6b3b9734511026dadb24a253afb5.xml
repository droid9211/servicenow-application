        <name>Calculate total price</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }
	var orderSelected = g_form.getValue('u_related_order_lines');
    var ga = new GlideAjax('getTotalPrice');
	
	g_form.addInfoMessage('orderSelected');
    ga.addParam('sysparm_name', 'getPrice');
    ga.addParam('sysparm_orderSelected', orderSelected);
    ga.getXML(setPrice);



    function setPrice(response) {

        var answer = response.responseXML.documentElement.getAttribute("answer");
        var parsedBody = JSON.parse(answer);
        g_form.setValue('final_product_price', answer);

    }
    //Type appropriate comment here, and begin script below

}]]></script>
